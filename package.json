{
  "name": "ggt-news-backend",
  "version": "0.0.0",
  "description": "description",
  "author": "cheft",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "migrate:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "predeploy:init": "rimraf dist && tsc",
    "deploy:init": "serverless deploy -v",
    "predeploy": "rimraf dist && tsc",
    "deploy": "serverless deploy function -f index",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^5.3.10",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.2.2",
    "ali-oss": "^6.0.1",
    "aws-serverless-express": "^3.3.5",
    "axios": "^1.6.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "qs": "^6.5.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.1",
    "snyk": "^1.230.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "snyk": true
}
